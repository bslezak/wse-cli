{
    "chmod": "0755",
    "directories": [
        "src",
        "app"
    ],
    "files": [
        "README.md",
        "LICENSE",
        "app/autoload.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        },
        {
            "name": "*.xml",
            "in": "vendor"
        },
        {
            "name": "*.xsd",
            "exclude": ["Tests"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "bin/console",
    "output": "wse_cli.phar",
    "stub": true
}